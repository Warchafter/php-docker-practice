# Ignore .env files that contain sensitive information like database passwords
.env

# Ignore Docker files and folders that may contain sensitive or generated data
docker-compose.override.yml
docker-compose.local.yml
docker-compose*.log

# Ignore Docker volumes or temporary data stored inside containers
docker-data/
docker-volumes/
*.sql
*.log
*.pid

# Ignore Composer dependencies
/vendor/
/composer.lock

# Ignore Node.js dependencies (if using any frontend libraries)
node_modules/
npm-debug.log
yarn-error.log
package-lock.json

# Ignore PHPStorm / IDE settings
.idea/
*.sublime-workspace
*.sublime-project

# Ignore cache and temporary files generated by PHP or frameworks
/cache/
/logs/
/tmp/

# Ignore files generated by PHPUnit (if using for testing)
.phpunit.result.cache

# Ignore uploaded or generated files
/public/uploads/

# Ignore system files and directories
.DS_Store
Thumbs.db

# Ignore backups and local environment files
*.bak
*.swp
*.swo

# Ignore Composer PHAR file if downloaded locally
composer.phar
